        	"ioti/silverstripe-blogcategories": "*",
        	"thezenmonkey/enhancedblog": "dev-patch-3 as dev-master",
		"silverstripe/spamprotection": "*",
		"tractorcow/silverstripe-akismet": "3.1.*@dev",
		"simplepie/simplepie": "*",
		"ezyang/htmlpurifier": "4.*"
    },
    "extra": {
                "installer-name": "blogging-recipe"
        }
